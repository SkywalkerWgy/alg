        loop invariant i_0: \forall integer p, q; 1 <= p < i && 1 <= q <= n ==> (s[p - 1] == t[q - 1] && dp[p][q] == dp[p - 1][q - 1] + 1) || is_max(dp[p - 1][q], dp[p][q - 1], dp[p][q]);
        loop invariant i_1: \forall integer k; 0 <= k <= n ==> dp[0][k] == 0;
        loop invariant i_2: \forall integer k; 0 <= k <= m ==> dp[k][0] == 0;
        loop invariant i_3: 1 <= i <= m + 1;

        loop assigns dp[1..(m)][1..(n)], i, j;
    */
    for (i = 1; i <= m; i++) {
        // Loop B
        /*@
            loop invariant i_4: \forall integer q; 1 <= q < j ==> (s[i - 1] == t[q - 1] && dp[i][q] == dp[i - 1][q - 1] + 1) || is_max(dp[i - 1][q], dp[i][q - 1], dp[i][q]);
            loop invariant i_5: \forall integer k; 0 <= k <= n ==> dp[0][k] == 0;
            loop invariant i_6: \forall integer k; 0 <= k <= m ==> dp[k][0] == 0;
            loop invariant i_7: 1 <= j <= n + 1;
            loop invariant i_8: \forall integer p, q; 1 <= p < i && 1 <= q <= n ==> (s[p - 1] == t[q - 1] && dp[p][q] == dp[p - 1][q - 1] + 1) || is_max(dp[p - 1][q], dp[p][q - 1], dp[p][q]);

            loop assigns dp[i][1..(n)], j;
        */
        for (j = 1; j <= n; j++) {
            if (s[i - 1] == t[j - 1]){
                dp[i][j] = dp[i - 1][j - 1] + 1;
            }
            else{
                dp[i][j] = max(dp[i - 1][j], dp[i][j - 1]);
            }
        }
    }
    return dp[m][n];
}
