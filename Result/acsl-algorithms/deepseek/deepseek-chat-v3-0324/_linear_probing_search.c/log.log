    predicate hashtable(int* a, integer length, integer key, integer data) =
        \exists integer i; 0 <= i < length && 
        key == (data + i) % length &&
        a[(data + i) % length] == data && 
        (\forall integer key; 0 <= key < i ==> a[(data + i) % length] != (0 - 1));
