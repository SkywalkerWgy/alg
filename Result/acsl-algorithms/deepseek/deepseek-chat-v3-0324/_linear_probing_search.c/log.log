*/
int _linear_probing_search(int table[], int size, int data) {
    int index = data % size;

    /*@
        loop assigns index;
    */
    while (table[index] != data && index < size) {
        index = index + 1;
    }
    if (index == size) {
        return -1;
    }
    else if (table[index] == data){
        return index;
    }
}
