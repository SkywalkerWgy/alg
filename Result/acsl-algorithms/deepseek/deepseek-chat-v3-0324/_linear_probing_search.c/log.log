//@ lemma mod_property: \forall integer x, y; x > 0 && y > 0 ==> 0 <= x % y < y;

/*@
    requires \valid_read(table+(0..(size - 1)));
    requires size > 0;
    requires data > 0;
    assigns \nothing;
    ensures e_1: \result == -1 || table[\result] == data;
