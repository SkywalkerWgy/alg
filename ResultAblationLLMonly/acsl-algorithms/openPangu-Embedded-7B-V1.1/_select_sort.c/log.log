    for (int i = 0; i < n - 1; i++)
    {
        int min = i;
        /*@
            loop assigns j, min;
        */
        for (int j = i; j < n; j++){  
            if (arr[j] < arr[min]){
                min = j;    
            }
        } 
        swap(&arr[min], &arr[i]);   
    }
}
