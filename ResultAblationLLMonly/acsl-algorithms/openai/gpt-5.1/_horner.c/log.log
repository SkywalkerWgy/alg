int _horner(int coeff[], int degree, int x) {
    int res = coeff[degree];
    /*@
        loop assigns i;
        loop assigns res;
    */
    for (int i = degree - 1; i >= 0; i--) {
        res = coeff[i] + x * res;
    }
    return res;
