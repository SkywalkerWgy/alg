        char c1 = s[i];
        // Loop B
        /*@
            loop invariant i_20: \forall integer p, q; i < p < n && p <= q < n ==> dp[p][q] == 0;

            loop invariant i_21: \forall integer k; i <= k < n ==> dp[k][k] == 1;

            loop invariant i_22: \forall integer p, q; i <= p < j && p < q < n ==> (s[p] == s[q] && dp[p][q] == dp[p + 1][q - 1] + 2) || (is_max(dp[p + 1][q], dp[p][q - 1], dp[p][q]));

            loop invariant i_23: \forall integer k; i < k <= j ==> (s[i] == s[k] && dp[i][k] == dp[i + 1][k - 1] + 2) || (is_max(dp[i + 1][k], dp[i][k - 1], dp[i][k]));


            loop assigns dp[i][(i + 1)..(n - 1)], j;
        */
        for (int j = i + 1; j < n; j++) {
            char c2 = s[j];
            if (c1 == c2) {
                dp[i][j] = dp[i + 1][j - 1] + 2;
            } else {
                dp[i][j] = max(dp[i + 1][j], dp[i][j - 1]);
            }
        }
    }
    return dp[0][n - 1];
}
